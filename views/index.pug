extends layout

block content
  h1= "Share your thinking self"
  a(href="/new") 
    button.btn.btn-default(type="button", data-dismiss="modal") Share something
  h2=title
  - for (var i = 0; i < messages.length; i++)
    li= messages[i].text + '. ' + messages[i].user + '. Added at ' + messages[i].added
    p= ''